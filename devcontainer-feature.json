{
    "name": "Dart",
    "id": "dart-sdk",
    "legacyIds": ["dart"],
    "version": "1.0.0",
    "description": "Install the Dart SDK",
    "documentationURL": "https://devcontainers.community/features-dart-sdk/",
    "containerEnv": {
        "DART_HOME": "/opt/dart-sdk",
        "DART_ROOT": "$DART_HOME",
        "PATH": "$PATH:$DART_HOME/bin:$PUB_CACHE/bin"
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dart-code.dart-code"
            ]
        }
    },
    "options": {
        "version": {
            "type": "string",
            "description": "Dart SDK version",
            "proposals": ["3.0.7"],
            "default": "3.0.7"
        }
    }
}
